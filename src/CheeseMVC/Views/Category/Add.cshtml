@model CheeseMVC.ViewModels.AddCategoryViewModel
@{
    ViewData["Title"] = "Add";
}

<form asp-controller="Category" asp-action="Add">
	<div class="form-group">
		<label asp-for="Name"></label>
		<input class="form-control" asp-for="Name" /><span asp-validation-for="Name"></span>
	</div>
	<input type="submit" value="Add Category"/>
</form>

